<h1>Posts</h1>

<%= link_to 'New Post', new_post_path %>

<table>
    <tr>
        <th>Id</th>
        <th>File</th>
        <th>Body</th>
        <th>Author</th>
        <th>Created_at</th>
        <th>Updated_at</th>
        <th>Actions</th>
    </tr>

    <tbody>
        <% @posts.each do |post| %>
            <tr>
                <td><%= post.id %></td>
                <td><%= post.title %></td>
                <td><%= post.body %></td>
                <td><%= post.author %></td>
                <td><%= post.created_at %></td>
                <td><%= post.updated_at %></td>
                <td>
                    <%= link_to 'Show', post_path(post) %>
                    <%= link_to 'Edit', edit_post_path(post) %>
                </td>
            </tr>
        <% end %> 
    </tbody>
</table>